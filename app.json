{
  "expo": {
    "name": "smartstudy",
    "slug": "smartstudy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0FBFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maheshwaran777.smartstudy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F0FBFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.maheshwaran777.smartstudy"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
